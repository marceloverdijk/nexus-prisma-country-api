datasource db {
  provider = "sqlite"
  url = "***"
}

generator prisma_client {
  provider = "prisma-client-js"
}

model Continent {
  id        String    @id
  code      String    @unique
  name      String    @unique
  countries Country[]
}

model Country {
  id          String    @id
  code        String    @unique
  name        String    @unique
  continentId String 
  continent   Continent @relation(fields: [continentId], references: [id])
}
